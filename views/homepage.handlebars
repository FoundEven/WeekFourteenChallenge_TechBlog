<div class="container-fluid postit">
    {{#each posts as |post|}}
    <div class="container-fluid">
        <div class="container-fluid">
            <h2>{{post.title}}</h2>
        </div>
        <p class="container-fluid">
            {{post.description}}
        </p>
    </div>
    <div class="container-fluid">
        <p>
            Post made by:
        </p>
    </div>
    {{/each}}
</div>